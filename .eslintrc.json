{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "eqeqeq": "off",
        "camelcase": "off",
        "no-case-declarations": "off",
        "prefer-const":"off",
        "no-var": "off",
        "consistent-return": "off",
        "vars-on-top": "off",
        "max-len": "off",
        "no-unused-vars": "off"
    }
}
